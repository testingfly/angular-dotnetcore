    "@angular/platform-browser-dynamic": "~16.2.12",
    "@angular/router": "~16.2.12",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "*"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "*",
    "@angular/cli": "*",
    "@angular/compiler-cli": "*",
    "@playwright/test": "^1.39.0",
    "@types/jasmine": "~5.1.1",
    "@types/node": "^20.8.10",
    "allure-playwright": "^2.9.2",
    "cross-env": "^7.0.3",
    "jasmine-core": "~5.1.1",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
