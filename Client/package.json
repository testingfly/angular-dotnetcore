{
  "name": "angular-aspnet-core-seed",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "serve": "ng serve -o",
    "start": "ng serve -o",
    "build": "ng build",
    "build:w": "ng build --watch",
